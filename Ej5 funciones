using System;
					
public class Program
{
	public static void Main()
	{
		int vidafinal;
		
		int vida, nivel;
		
		Console.WriteLine("Introduce la vida: ");
		vida = LeerEntero();
		Console.WriteLine("Introduce el nivel: ");
		nivel = LeerEntero();
		vidafinal = Vidafinal(vida, nivel);
		Console.WriteLine("La vida final es: " + vidafinal);
		
	}
	
	static int Vidafinal(int vida, int nivel)
	{
		int vidafinal;
		vidafinal = vida * nivel;
		return vidafinal;
	}
	static int LeerEntero()
	{
		int numeroLeido;
		numeroLeido = Convert.ToInt32(Console.ReadLine());
		return numeroLeido;
}
}
