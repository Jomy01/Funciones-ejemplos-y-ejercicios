using System;
					
public class Program
{
	public static void Main()
	{
		int numero1;
		int numero2;
		
		Console.WriteLine("Introduc el primer número: ");
		numero1 = LeerEntero();
		Console.WriteLine("Introduce el segundo numero: ");
		numero2 = LeerEntero();
		
		if ( numero1 % numero2 == 0)
		{
			Console.WriteLine("Los números son divisibles entre sí");
		}
		else
		{
			Console.WriteLine("Los números NO son divisibles entre sí");
		}
	}
		
		
		static int LeerEntero()
	{
		int numeroLeido;
		numeroLeido = Convert.ToInt32(Console.ReadLine());
		return numeroLeido;
}
		
	}
